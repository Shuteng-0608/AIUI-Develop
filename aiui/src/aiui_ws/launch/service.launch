<launch>
    <!-- 启动 TTS 节点 -->
    <node 
        pkg="aiui" 
        type="aiui_tts_service_node.py" 
        name="aiui_tts_service_node" 
        output="screen"
    />

    <!-- 启动 VLM 节点 -->
    
    <node 
        pkg="aiui" 
        type="vlm_service_node.py"
        name="vlm_service_node" 
        output="screen"
    />

    <!-- 启动 VLM 节点 -->
    
    <node 
        pkg="aiui" 
        type="dh5_control_ros.py"
        name="dh5_service_node" 
        output="screen"
    />
    
    <!-- 启动 OrbbecSDK_ROS1 节点 -->
